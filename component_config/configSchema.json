{
    "type": "object",
    "title": "extractor configuration",
    "required": [
        "email",
        "#api_token",
        "sub_domain"
    ],
    "properties": {
        "email": {
            "type": "string",
            "title": "Email",
            "propertyOrder": 1
        },
        "#api_token": {
            "type": "string",
            "title": "API token",
            "format": "password",
            "propertyOrder": 2
        },
        "sub_domain": {
            "type": "string",
            "title": "Sub domain",
            "propertyOrder": 3
        },
        "custom_bucket": {
            "type": "boolean",
            "title": "Custom output bucket",
            "format": "checkbox",
            "description": "By default bucket will be created",
            "propertyOrder": 4
        },
        "destination_bucket": {
          "type": "string",
          "title": "Destination bucket",
          "description": "Choice of destination bucket",
          "enum": [],
          "format": "select",
          "options": {
            "async": {
              "label": "Load available buckets",
              "action": "get_buckets"
            },
            "dependencies": {
              "custom_bucket": true
            }
          },
          "uniqueItems": true,
          "propertyOrder": 5,
          "minItems": 1,
          "maxItems": 1
        },
        "debug": {
            "type": "boolean",
            "title": "Debug Logging",
            "format": "checkbox",
            "propertyOrder": 6
        }
    }
}
